<editors>
<!-- Boolean -->
	<editor id="bool">
		<range uom="2" subset="0,1" />
	</editor>

	<editor id="tempf">
		<range uom="17" min="-40" max="149" step="1" prec="1" />
	</editor>
	
	<editor id="humidity">
		<range uom="22" min="10" max="100" prec="0" />
	</editor>
	
	<editor id="inhg">
		<range uom="23" min="8.25" max="32.50" prec="2" />
	</editor>
	
	<editor id="lux">
		<range uom="36" min="0" max="200000" prec="0" />
	</editor>	

	<editor id="uv">
		<range uom="71" min="0" max="20" prec="1" />
	</editor>
	
	<editor id="solarradiation">
		<range uom="74" min="0" max="200000" prec="1" />
	</editor>
	
	<editor id="mph">
		<range uom="48" min="0" max="100" prec="1" />
	</editor>
	
	<!--<editor id="kph">
		<range uom="32" min="0" max="500" prec="1" />
	</editor>
	-->
	
	<editor id="inches">
		<range uom="105" min="0" max="288" prec="3" />
	</editor>
	
	<editor id="WINDDIR">
		<range uom="76" min="0" max="360" prec="1" />
	</editor>

</editors>