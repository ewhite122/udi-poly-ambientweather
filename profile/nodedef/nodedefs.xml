<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller -->
    <nodeDef id="controller" nls="ctl">
        <editors />
        <sts>
			<st id="ST" editor="bool" />
		</sts>
        <cmds>
            <sends />
            <accepts>
              <cmd id="DISCOVER" />
              <cmd id="REMOVE_NOTICES_ALL" />
              <cmd id="UPDATE_PROFILE" />
            </accepts>
        </cmds>
    </nodeDef>
	<!-- pwsnodetype -->
    <nodeDef id="pwsnodetype" nls="pwsnode">
        <editors />
        <sts>
            <st id="ST" editor="bool" hide=True />
            <st id="CLITEMP" editor="CLITEMP" /> <!--Outside Temp -->
            <st id="GV1" editor="CLITEMP" /> <!--Inside Temp -->
            <st id="CLIHUM" editor="CLIHUM" /> <!--Outside Humidity -->
            <st id="GV3" editor="CLIHUM" /> <!--Inside Humidity -->
            <st id="BARPRES" editor="INHG" /> <!--Rel Barometric Pressure -->
            <st id="ATMPRES" editor="INHG" /> <!--Abs Barometric Pressure -->
            <st id="LUMIN" editor="LUMIN" /> <!--Luminance -->
            <st id="UV" editor="UV" /> <!--UV -->
            <st id="SOLRAD" editor="SOLRAD" /> <!--Solar Radiation -->
            <st id="GV9" editor="INCH" /> <!--Hourly Rain Inches -->
            <st id="GV10" editor="INCH" /> <!--Daily Rain Inches -->
            <st id="GV11" editor="INCH" /> <!--Weekly Rain Inches -->
            <st id="GV12" editor="INCH" /> <!--Monthly Rain Inches -->
            <st id="GV13" editor="INCH" /> <!--Yearly Rain Inches -->
            <st id="GV14" editor="INCH" /> <!--Total Rain Inches -->
            <st id="WINDDIR" editor="WINDDIR" /> <!--Wind Direction -->
            <st id="GV16" editor="I_WIND_DIRECTION" /> <!--Wind Direction -->
            <st id="SPEED" editor="MPH" /> <!--Wind Speed MPH -->
            <st id="GV17" editor="MPH" /> <!--Wind Gust MPH -->
            <st id="GV18" editor="MPH" /> <!--Wind Max Daily Gust -->
            <st id="GV19" editor="CLITEMP" /> <!--Computed feels like temperture -->
            <st id="GV20" editor="CLITEMP" /> <!--Computed dew point temperture -->
        </sts>
        <cmds>
            <sends />
            <accepts>
                <!--<cmd id="DON" />
                <cmd id="DOF" /> -->
            </accepts>
        </cmds>
    </nodeDef>
    <nodeDef id="addontype" nls="addonnode">
        <editors />
        <sts>
            <st id="ST" editor="bool" hide=True />
