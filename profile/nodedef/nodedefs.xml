            <st id="GV2" editor="CLITEMP" /> <!--Sensor 3 Temperature -->
            <st id="GV3" editor="CLITEMP" /> <!--Sensor 4 Temperature -->
            <st id="GV4" editor="CLITEMP" /> <!--Sensor 5 Temperature -->
            <st id="GV5" editor="CLITEMP" /> <!--Sensor 6 Temperature -->
            <st id="GV6" editor="CLITEMP" /> <!--Sensor 7 Temperature -->
            <st id="GV7" editor="CLITEMP" /> <!--sensor 8 Temperature -->
            <st id="GV8" editor="CLIHUM" /> <!--Sensor 1 Humidity -->
            <st id="GV9" editor="CLIHUM" /> <!--Sensor 2 Humidity -->
            <st id="GV10" editor="CLIHUM" /> <!--Sensor 3 Humidity -->
            <st id="GV11" editor="CLIHUM" /> <!--Sensor 4 Humidity -->
            <st id="GV12" editor="CLIHUM" /> <!--Sensor 5 Humidity -->
            <st id="GV13" editor="CLIHUM" /> <!--Sensor 6 Humidity -->
            <st id="GV14" editor="CLIHUM" /> <!--Sensor 7 Humidity -->
            <st id="GV15" editor="CLIHUM" /> <!--Sensor 8 Humidity -->
        </sts>
        <cmds>
            <sends />
            <accepts>
                <!--<cmd id="DON" />
                <cmd id="DOF" /> -->
            </accepts>
        </cmds>
    </nodeDef>



